{
  "version": 2,
  "projects": {
    "apollo-server-example": {
      "root": "packages/apollo-server-example",
      "sourceRoot": "packages/apollo-server-example/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/apollo-server-example",
            "main": "packages/apollo-server-example/src/main.ts",
            "tsConfig": "packages/apollo-server-example/tsconfig.app.json",
            "assets": ["packages/apollo-server-example/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "packages/apollo-server-example/src/environments/environment.ts",
                  "with": "packages/apollo-server-example/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "apollo-server-example:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/apollo-server-example/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/apollo-server-example"],
          "options": {
            "jestConfig": "packages/apollo-server-example/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "express": {
      "root": "packages/express",
      "sourceRoot": "packages/express/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/express/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/express"],
          "options": {
            "jestConfig": "packages/express/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/express",
            "tsConfig": "packages/express/tsconfig.lib.json",
            "packageJson": "packages/express/package.json",
            "main": "packages/express/src/index.ts",
            "assets": ["packages/express/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "apollo-server-example"
}
